{{#each importedSchemas}}
import type {{this}} from "./{{this}}";
{{/each}}
/**
 * {{{description}}}
 */
{{#if (eq type "object")}}
type {{name}} = {
  {{#each properties}}
  /**
   * {{{description}}}
   */
  {{{content}}}
  {{/each}}
};
{{else}}
type {{name}} = {{{definition}}};
{{/if}}

export default {{name}};
